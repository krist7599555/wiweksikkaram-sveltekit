{
    "name": "wiweksikkaram-sveltekit",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "wrangler dev .svelte-kit/cloudflare/_worker.js",
        "prepare": "bun run types && svelte-kit sync || echo ''",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "types": "bun run types:cf && bun run types:pb",
        "types:cf": "wrangler types",
        "types:pb": "pocketbase-typegen --env --out ./src/lib/pocketbase/pb_types.ts",
        "deploy": "bun run build && bun wrangler deploy"
    },
    "devDependencies": {
        "@aiquants/html-to-markdown": "^1.2.1",
        "@danielx/civet": "^0.11.1",
        "@eslint/compat": "^1.4.1",
        "@eslint/js": "^9.39.2",
        "@fontsource-variable/noto-serif-thai": "^5.2.7",
        "@poppanator/sveltekit-svg": "^6.0.1",
        "@sveltejs/adapter-cloudflare": "^7.2.4",
        "@sveltejs/kit": "^2.49.2",
        "@sveltejs/vite-plugin-svelte": "^6.2.1",
        "@tailwindcss/forms": "^0.5.11",
        "@tailwindcss/typography": "^0.5.19",
        "@tailwindcss/vite": "^4.1.18",
        "@types/bun": "^1.3.5",
        "@types/node": "^24.10.4",
        "arktype": "^2.1.29",
        "collect.js": "^4.36.1",
        "csv-parse": "^6.1.0",
        "daisyui": "^5.5.14",
        "es-toolkit": "^1.43.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-civet": "^0.0.6",
        "eslint-plugin-svelte": "^3.13.1",
        "globals": "^16.5.0",
        "htmlfy": "^1.0.1",
        "mediabunny": "^1.27.2",
        "pocketbase-typegen": "^1.3.2",
        "prettier": "^3.7.4",
        "prettier-plugin-svelte": "^3.4.1",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "remeda": "^2.33.0",
        "rss-to-json": "^2.1.1",
        "rxjs": "^7.8.2",
        "smol-toml": "^1.6.0",
        "string-similarity": "^4.0.4",
        "sugar": "^2.0.6",
        "svelte": "^5.46.1",
        "svelte-check": "^4.3.5",
        "svelte-mp3": "^1.3.0",
        "tailwindcss": "^4.1.18",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.50.1",
        "valibot": "^1.2.0",
        "vite": "^7.3.0",
        "vite-node": "^5.2.0",
        "vite-plugin-devtools-json": "^1.0.0",
        "wrangler": "^4.56.0",
        "zx": "^8.8.5"
    },
    "dependencies": {
        "pocketbase": "^0.26.5",
        "ts-pattern": "^5.9.0"
    }
}
